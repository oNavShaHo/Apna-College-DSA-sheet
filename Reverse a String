class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        
        String str1="";
        char c[]=str.toCharArray();
        for(int i=c.length-1;i>=0;i--)
        {
         str1+=c[i];
        }
        return str1;
    }
}
